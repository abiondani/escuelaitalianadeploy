{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": [
            "views/**",
            "data/**",
            "middlewares/**",
            "models/**",
            "controllers/**",
            "routes/**",
            "objects/**"
          ]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ],
    "framework": "nodejs"
  }
  